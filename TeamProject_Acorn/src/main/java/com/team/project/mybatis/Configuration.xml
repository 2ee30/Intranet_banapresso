<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
 <configuration>
 	<typeAliases>
 		<typeAlias type="com.team.project.board.dto.BoardDto" alias="boardDto"/>
 		<typeAlias type="com.team.project.board.dto.BoardCommentDto" alias="boardCommentDto"/>
		<typeAlias type="com.team.project.notice.dto.NoticeDto" alias="noticeDto"/>
		<typeAlias type="com.team.project.file.dto.FileDto" alias="fileDto"/>
